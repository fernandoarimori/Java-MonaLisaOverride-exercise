package Hierarchy;

public class ObjectCreation {

	public static void main(String[] args) {
		Animal x = new Animal();
		x.status();
		System.out.println("\n======================\n");
		
		Dog y = new Dog();
		y.status();
		
		System.out.println("\n======================\n");
		Horse z = new Horse();
		z.status();

	}

}
